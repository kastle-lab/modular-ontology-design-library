  rdfs:subClassOf [ owl:intersectionOf ( :TreeNode
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :hasParent ;
                                           owl:cardinality "1"^^xsd:nonNegativeInteger
                                         ]
                                       ) ;
                    rdf:type owl:Class
                  ]
] .


