[ owl:intersectionOf ( :TreeNode
                       [ rdf:type owl:Class ;
                         owl:complementOf :LeafNode
                       ]
                     ) ;
  rdf:type owl:Class ;
  rdfs:subClassOf [ owl:intersectionOf ( :TreeNode
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :hasChild ;
                                           owl:someValuesFrom :TreeNode
                                         ]
                                       ) ;
                    rdf:type owl:Class
                  ]
] .


[ owl:intersectionOf ( :TreeNode
                       [ rdf:type owl:Class ;
                         owl:complementOf :RootNode
                       ]
                     ) ;
